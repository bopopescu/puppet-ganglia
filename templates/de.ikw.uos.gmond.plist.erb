<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>de.ikw.uos.gmond</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/sbin/gmond</string>
		<string>-f</string>
		<string>--conf=<%= ganglia_monitor_conf %></string>
		<string>--pid-file=/var/run/gmond.pid</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>
